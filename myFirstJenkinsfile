pipeline {
    agent any
    stages {
        stage("Build") {
           steps {
              echo "Jenkinspipe build"
            }
        }
        stage("test") {
           steps {
              echo "Jenkinspipe test"
            }
        }
        stage("deploy") {
           steps {
              echo "My First Jenkinspipe deploy"
            }
        }
    }
}
